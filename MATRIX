#include<stdio.h>
int main()
{
	int matrix[3][3];
	int i,j;
	printf("Input elements in the matrix:\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("element-[%d],[%d]:",i,j);
			scanf("%d",&matrix[i][j]);
		}
	}
	printf("\n");
	printf("The matrix is:\n\n");
   for(i=0; i<3; i++) 
   {
      for(j=0;j<3;j++) 
	  {
         printf("%d  ", matrix[i][j]);
         if(j==2)
		 {
            printf("\n");
         }
      }
   }
   return 0;
}
